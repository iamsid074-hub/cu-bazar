{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_PUBLISHABLE_KEY": "@vite_supabase_publishable_key"
  },
  "routes": [
    {
      "src": "^/(?!.*\\.[a-z0-9]+$).*$",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}